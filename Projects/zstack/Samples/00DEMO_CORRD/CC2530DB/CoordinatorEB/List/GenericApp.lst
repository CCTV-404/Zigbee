    554              if ( keys & HAL_KEY_SW_1 )
    555              {
    556                // Since SW1 isn't used for anything else in this application...
    557          #if defined( SWITCH1_BIND )
    558                // we can use SW1 to simulate SW2 for devices that only have one switch,
    559                keys |= HAL_KEY_SW_2;
    560          #elif defined( SWITCH1_MATCH )
