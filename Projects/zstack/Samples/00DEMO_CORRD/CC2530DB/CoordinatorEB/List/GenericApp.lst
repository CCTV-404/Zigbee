    468          /*********************************************************************
    469           * @fn      GenericApp_ProcessZDOMsgs()
    470           *
    471           * @brief   Process response messages
    472           *
    473           * @param   none
    474           *
    475           * @return  none
    476           */
    477          static void GenericApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    478          {
    479            switch ( inMsg->clusterID )
    480            { 
    481              case End_Device_Bind_rsp:        
    482                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    483                {
    484                  // Light LED
    485                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    486                }
    487          #if defined( BLINK_LEDS )
    488                else
    489                {
    490                  // Flash LED to show failure
    491                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    492                }
    493          #endif
    494                break;
    495          
    496              case Match_Desc_rsp:
    497                {
    498                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    499                  if ( pRsp )
    500                  {
    501                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    502                    {
    503                      GenericApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    504                      GenericApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    505                      // Take the first endpoint, Can be changed to search through endpoints
    506                      GenericApp_DstAddr.endPoint = pRsp->epList[0];
    507          
    508                      // Light LED
    509                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    510                    }
    511                    osal_mem_free( pRsp );
    512                  }
    513                }
    514                break;
    515            }
    516          }
