    842          /*********************************************************************
    843           * @fn      GenericApp_ProcessRtosMessage
    844           *
    845           * @brief   Receive message from RTOS queue, send response back.
    846           *
    847           * @param   none
    848           *
    849           * @return  none
    850           */
    851          static void GenericApp_ProcessRtosMessage( void )
    852          {
    853            osalQueue_t inMsg;
    854          
    855            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    856            {
    857              uint8 cmndId = inMsg.cmnd;
    858              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    859          
    860              switch ( cmndId )
    861              {
    862                case CMD_INCR:
    863                  counter += 1;  /* Increment the incoming counter */
    864                                 /* Intentionally fall through next case */
    865          
    866                case CMD_ECHO:
    867                {
    868                  userQueue_t outMsg;
    869          
    870                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    871                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    872                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    873                  break;
    874                }
    875                
    876                default:
    877                  break;  /* Ignore unknown command */    
    878              }
    879            }
    880          }
    881          #endif
