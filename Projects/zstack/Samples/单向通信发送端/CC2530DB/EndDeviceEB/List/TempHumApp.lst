    403          /*********************************************************************
    404           * @fn      TempHumApp_HandleKeys
    405           *
    406           * @brief   Handles all key events for this device.
    407           *
    408           * @param   shift - true if in shift/alt.
    409           * @param   keys - bit field for key events. Valid entries:
    410           *                 HAL_KEY_SW_4
    411           *                 HAL_KEY_SW_3
    412           *                 HAL_KEY_SW_2
    413           *                 HAL_KEY_SW_1
    414           *
    415           * @return  none
    416           */
    417          static void TempHumApp_HandleKeys( uint8 shift, uint8 keys )
    418          {
    419            zAddrType_t dstAddr;
    420          
    421            // Shift is used to make each button/switch dual purpose.
    422            if ( shift )
    423            {
    424              if ( keys & HAL_KEY_SW_1 )
    425              {
    426              }
    427              if ( keys & HAL_KEY_SW_2 )
    428              {
    429              }
    430              if ( keys & HAL_KEY_SW_3 )
    431              {
    432              }
    433              if ( keys & HAL_KEY_SW_4 )
    434              {
    435              }
    436            }
    437            else
    438            {
    439              if ( keys & HAL_KEY_SW_1 )
    440              {
    441                // Since SW1 isn't used for anything else in this application...
    442          #if defined( SWITCH1_BIND )
    443                // we can use SW1 to simulate SW2 for devices that only have one switch,
    444                keys |= HAL_KEY_SW_2;
    445          #elif defined( SWITCH1_MATCH )
    446                // or use SW1 to simulate SW4 for devices that only have one switch
    447                keys |= HAL_KEY_SW_4;
    448          #endif
    449              }
    450          
    451              if ( keys & HAL_KEY_SW_2 )
    452              {
    453                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    454          
    455                // Initiate an End Device Bind Request for the mandatory endpoint
    456                dstAddr.addrMode = Addr16Bit;
    457                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    458                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    459                                      TempHumApp_epDesc.endPoint,
    460                                      TEMPHUMAPP_PROFID,
    461                                      TEMPHUMAPP_MAX_CLUSTERS, (cId_t *)TempHumApp_ClusterList,
    462                                      TEMPHUMAPP_MAX_CLUSTERS, (cId_t *)TempHumApp_ClusterList,
    463                                      FALSE );
    464              }
    465          
    466              if ( keys & HAL_KEY_SW_3 )
    467              {
    468              }
    469          
    470              if ( keys & HAL_KEY_SW_4 )
    471              {
    472                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    473                // Initiate a Match Description Request (Service Discovery)
    474                dstAddr.addrMode = AddrBroadcast;
    475                dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
    476                ZDP_MatchDescReq( &dstAddr, NWK_BROADCAST_SHORTADDR,
    477                                  TEMPHUMAPP_PROFID,
    478                                  TEMPHUMAPP_MAX_CLUSTERS, (cId_t *)TempHumApp_ClusterList,
    479                                  TEMPHUMAPP_MAX_CLUSTERS, (cId_t *)TempHumApp_ClusterList,
    480                                  FALSE );
    481              }
    482            }
    483          }
