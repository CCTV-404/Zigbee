    559          /*********************************************************************
    560           * @fn      GAS_ProcessRtosMessage
    561           *
    562           * @brief   Receive message from RTOS queue, send response back.
    563           *
    564           * @param   none
    565           *
    566           * @return  none
    567           */
    568          static void GAS_ProcessRtosMessage( void )
    569          {
    570            osalQueue_t inMsg;
    571          
    572            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    573            {
    574              uint8 cmndId = inMsg.cmnd;
    575              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    576          
    577              switch ( cmndId )
    578              {
    579                case CMD_INCR:
    580                  counter += 1;  /* Increment the incoming counter */
    581                                 /* Intentionally fall through next case */
    582          
    583                case CMD_ECHO:
    584                {
    585                  userQueue_t outMsg;
    586          
    587                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    588                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    589                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    590                  break;
    591                }
    592                
    593                default:
    594                  break;  /* Ignore unknown command */    
    595              }
    596            }
    597          }
    598          #endif
