    530          static void GAS_SendTheMessage( void )
    531          {
    532            char theMessageData[] = "Hello World";
    533            
    534            P1DIR &= ~(1<<3);
    535            
    536            if(GASIN == 1)
    537            {
    538              theMessageData[0] = 'Y';
    539            } else {
    540              theMessageData[0] = 'N';
    541            }
    542          
    543            if ( AF_DataRequest( &GAS_DstAddr, &GAS_epDesc,
    544                                 GAS_CLUSTERID,
    545                                 (byte)osal_strlen( theMessageData ) + 1,
    546                                 (byte *)&theMessageData,
    547                                 &GAS_TransID,
    548                                 AF_DISCV_ROUTE, AF_DEFAULT_RADIUS ) == afStatus_SUCCESS )
    549            {
    550              // Successfully requested to be sent.
    551            }
    552            else
    553            {
    554              // Error occurred in request to send.
    555            }
    556          }
