    409          /*********************************************************************
    410           * @fn      GAS_HandleKeys
    411           *
    412           * @brief   Handles all key events for this device.
    413           *
    414           * @param   shift - true if in shift/alt.
    415           * @param   keys - bit field for key events. Valid entries:
    416           *                 HAL_KEY_SW_4
    417           *                 HAL_KEY_SW_3
    418           *                 HAL_KEY_SW_2
    419           *                 HAL_KEY_SW_1
    420           *
    421           * @return  none
    422           */
    423          static void GAS_HandleKeys( uint8 shift, uint8 keys )
    424          {
    425            zAddrType_t dstAddr;
    426          
    427            // Shift is used to make each button/switch dual purpose.
    428            if ( shift )
    429            {
    430              if ( keys & HAL_KEY_SW_1 )
    431              {
    432              }
    433              if ( keys & HAL_KEY_SW_2 )
    434              {
    435              }
    436              if ( keys & HAL_KEY_SW_3 )
    437              {
    438              }
    439              if ( keys & HAL_KEY_SW_4 )
    440              {
    441              }
    442            }
    443            else
    444            {
    445              if ( keys & HAL_KEY_SW_1 )
    446              {
    447                // Since SW1 isn't used for anything else in this application...
    448          #if defined( SWITCH1_BIND )
    449                // we can use SW1 to simulate SW2 for devices that only have one switch,
    450                keys |= HAL_KEY_SW_2;
    451          #elif defined( SWITCH1_MATCH )
    452                // or use SW1 to simulate SW4 for devices that only have one switch
    453                keys |= HAL_KEY_SW_4;
    454          #endif
    455              }
    456          
    457              if ( keys & HAL_KEY_SW_2 )
    458              {
    459                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    460          
    461                // Initiate an End Device Bind Request for the mandatory endpoint
    462                dstAddr.addrMode = Addr16Bit;
    463                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    464                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    465                                      GAS_epDesc.endPoint,
    466                                      GAS_PROFID,
    467                                      GAS_MAX_CLUSTERS, (cId_t *)GAS_ClusterList,
    468                                      GAS_MAX_CLUSTERS, (cId_t *)GAS_ClusterList,
    469                                      FALSE );
    470              }
    471          
    472              if ( keys & HAL_KEY_SW_3 )
    473              {
    474              }
    475          
    476              if ( keys & HAL_KEY_SW_4 )
    477              {
    478                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    479                // Initiate a Match Description Request (Service Discovery)
    480                dstAddr.addrMode = AddrBroadcast;
    481                dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
    482                ZDP_MatchDescReq( &dstAddr, NWK_BROADCAST_SHORTADDR,
    483                                  GAS_PROFID,
    484                                  GAS_MAX_CLUSTERS, (cId_t *)GAS_ClusterList,
    485                                  GAS_MAX_CLUSTERS, (cId_t *)GAS_ClusterList,
    486                                  FALSE );
    487              }
    488            }
    489          }
