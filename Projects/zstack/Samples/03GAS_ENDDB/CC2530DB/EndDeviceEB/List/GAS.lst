    359          /*********************************************************************
    360           * @fn      GAS_ProcessZDOMsgs()
    361           *
    362           * @brief   Process response messages
    363           *
    364           * @param   none
    365           *
    366           * @return  none
    367           */
    368          static void GAS_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    369          {
    370            switch ( inMsg->clusterID )
    371            {
    372              case End_Device_Bind_rsp:
    373                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    374                {
    375                  // Light LED
    376                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    377                }
    378          #if defined( BLINK_LEDS )
    379                else
    380                {
    381                  // Flash LED to show failure
    382                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    383                }
    384          #endif
    385                break;
    386          
    387              case Match_Desc_rsp:
    388                {
    389                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    390                  if ( pRsp )
    391                  {
    392                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    393                    {
    394                      GAS_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    395                      GAS_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    396                      // Take the first endpoint, Can be changed to search through endpoints
    397                      GAS_DstAddr.endPoint = pRsp->epList[0];
    398          
    399                      // Light LED
    400                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    401                    }
    402                    osal_mem_free( pRsp );
    403                  }
    404                }
    405                break;
    406            }
    407          }
