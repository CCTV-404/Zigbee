    555          /*********************************************************************
    556           * @fn      TempHumApp_ProcessRtosMessage
    557           *
    558           * @brief   Receive message from RTOS queue, send response back.
    559           *
    560           * @param   none
    561           *
    562           * @return  none
    563           */
    564          static void TempHumApp_ProcessRtosMessage( void )
    565          {
    566            osalQueue_t inMsg;
    567          
    568            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    569            {
    570              uint8 cmndId = inMsg.cmnd;
    571              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    572          
    573              switch ( cmndId )
    574              {
    575                case CMD_INCR:
    576                  counter += 1;  /* Increment the incoming counter */
    577                                 /* Intentionally fall through next case */
    578          
    579                case CMD_ECHO:
    580                {
    581                  userQueue_t outMsg;
    582          
    583                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    584                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    585                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    586                  break;
    587                }
    588                
    589                default:
    590                  break;  /* Ignore unknown command */    
    591              }
    592            }
    593          }
    594          #endif
